scopeName: 'log.output'
name: 'log-output'
patterns:
    - name: "log.info"
      match: "\\b(INFO)\\b"
    - name: "log.warning"
      match: "\\b(WARN)\\b"
    - name: "log.error"
      match: "\\b(ERROR)\\b"
    - name: "strong log.error"
      match: "\\b(FATAL)\\b"
    - name: "constant.language"
      match: "\\b([0-9]+\.?[0-9]+|true|false|null)\\b"
    - name: "log.string"
      match: "\"[^\"]*\""
    - name: "log.string"
      match: "(?<![\w])'[^']*'"
